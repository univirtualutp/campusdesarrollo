{{!
    @template  theme_boosted/footer
    @copyright 2022-2023 koditik.com

    Page footer.

    Example context (json):
    {
        "output": {
            "page_doc_link": "Help and documentation",
            "supportemail": "<a href=\"#\">Contact site support</a>",
            "has_popover_links": true,
            "services_support": "Services and support",
            "login_info": "You are logged in as cute kitten",
            "moodle_release": "90210"
        }
    }
}}



<footer id="page-footer" class="container-fluid is-mobile">
  <div class="footer-blocks p-2">
      <div class="row">
        <div class="col-md footer-block-1">
            <h3>{{{footerblocktitle1}}}</h3>
            <p>{{{footerblockcontent1}}}</p>
        </div>

        <div class="col-md footer-block-2">
            <h3>{{{footerblocktitle2}}}</h3>
            <p>{{{footerblockcontent2}}}</p>
        </div>

        <div class="col-md footer-block-3">
            <h3>{{{footerblocktitle3}}}</h3>
            <p>{{{footerblockcontent3}}}</p>
        </div>

        <div class="col-md footer-block-4">
            <h3>{{{footerblocktitle4}}}</h3>
            <p>{{{footerblockcontent4}}}</p>
        </div>
     </div>    
  </div>   

  <div class="footer-icons row">
    <div class="col text-center">
        {{{socialicons}}}
    </div>
  </div>

  <div class="footer-footnote row">
      <div class="col text-center">
          <p>{{{footernote}}}</p>
      </div>
  </div>

  <div class="footer-content-debugging row">
      <div class="col">
        {{{ output.debug_footer_html }}}
      </div>
  </div>

  {{{output.standard_footer_html}}}
</footer>

{{{ output.standard_end_of_body_html }}}



